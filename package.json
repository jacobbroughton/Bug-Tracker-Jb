{
  "name": "bug-tracker",
  "version": "1.0.0",
  "description": "This bug tracker allows users to log and keep track of errors that pop up during development.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm start --prefix client",
